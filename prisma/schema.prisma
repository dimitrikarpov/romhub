generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Rom {
  id          String  @id @default(cuid())
  title       String
  file        String
  description String?
  crc32       String
  sha1        String  @unique
  tags        String
  images      String?
}

model Tag {
  id   String @id @default(cuid())
  name String @unique
}
